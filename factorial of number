#include<stdio.h>  
int main()    
{    
 int i,fact=1,num;    
 printf("Enter a number: ");    
 scanf("%d",&num);
 if(num<0)
 {
     printf("Factorial cant be done to negative number");
 }
 else
 {
    for(i=1;i<=num;i++)
    {
      fact=fact*i;    
    }    
    printf("Factorial of %d is: %d",num,fact);
 }
return 0;  
}
