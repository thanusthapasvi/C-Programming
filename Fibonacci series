#include <stdio.h>
main( )
{
    int i,n,f1=0,f2=1;
    int fib=f1+f2;
    printf("enter the number of terms: ");
    scanf("%d",&n);
    printf("fibonacci series: %d\t%d",f1,f2);
    for(i=3;i<=n;++i)
    {
        printf("\t%d",fib);
        f1=f2;
        f2=fib;
        fib=f1+f2;
    } return 0;
}
